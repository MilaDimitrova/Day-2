  
#include <stdio.h>
#include <stdlib.h>

int maximum(int* arr, int n) {
    int max = 0;
    for(int i = 0; i < n; i++) {
        if(arr[i] > max) {
            max = arr[i];
        }
    }
    return max;
}

int main() {
    int a[] = {1,4,5,-1,3,5};
    int max = maximum(a, 6);
    printf("Max: %d", max);
}

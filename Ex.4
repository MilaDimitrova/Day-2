#include <stdio.h>
#include <stdlib.h>


int minimum(int* arr, int n) {
    int min = 0;
    for(int i = 0; i < n; i++) {
        if(arr[i] < min) {
            min = arr[i];
        }
    }
    return min;
}

int main() {
    int a[] = {1,4,5,-1,3,5};
    int min = minimum(a, 6);
    printf("Min: %d", min);
}
